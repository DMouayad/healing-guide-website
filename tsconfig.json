{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@localization/*": [
        "src/i18n/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@data/*": [
        "src/common/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
      "@scripts/*": [
        "src/assets/scripts/*"
      ],
      "@styles/*": [
        "src/assets/css/*"
      ],
      "@common/*": [
        "src/common/*"
      ]
    },
  }
}